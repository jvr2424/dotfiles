{
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-p>": false,
        "<C-w>": false
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "security.workspace.trust.untrustedFiles": "open",
    "jupyter.askForKernelRestart": false,
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "window.commandCenter": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "notebook.diff.ignoreOutputs": true,
    "notebook.diff.ignoreMetadata": true,
    // Enable formatting the entire Notebook on save
      "notebook.formatOnCellExecution": true,
      // Run the enabled code actions on the entire Notebook on save
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "interactiveWindow.executeWithShiftEnter": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vscode-neovim.neovimExecutablePaths.darwin": "/Users/jracaniell/.local/share/bob/v0.9.5/bin/nvim",
    "ruff.nativeServer": "on",
    "python.analysis.nodeExecutable": "/opt/homebrew/bin/node",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "notebook.lineNumbers": "on",
    "editor.lineNumbers": "relative",
    "editor.acceptSuggestionOnEnter": "off",
    "cSpell.enabledFileTypes": {
        "*": false
    },
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
}
